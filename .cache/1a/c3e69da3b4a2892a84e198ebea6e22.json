ource":"components/Block.js","original":{"line":58,"column":9}},{"name":"displayTransaction","generated":{"line":102,"column":202},"source":"components/Block.js","original":{"line":58,"column":14}},{"generated":{"line":102,"column":220},"source":"components/Block.js","original":{"line":55,"column":6}},{"generated":{"line":102,"column":221},"source":"components/Block.js","original":{"line":54,"column":4}},{"generated":{"line":103,"column":0},"source":"components/Block.js","original":{"line":61,"column":3}},{"name":"Component","generated":{"line":107,"column":2},"source":"components/Block.js","original":{"line":5,"column":20}},{"name":"Block","generated":{"line":109,"column":15},"source":"components/Block.js","original":{"line":64,"column":15}}],"sources":{"components/Block.js":"import React, { Component } from 'react';\nimport { Button } from 'react-bootstrap';\nimport Transaction from './Transaction';\n\nclass Block extends Component {\n  state = { displayTransaction: false };\n  toggleTransaction = () => {\n    this.setState({ displayTransaction: !this.state.displayTransaction });\n  };\n  get displayTransaction() {\n    const { data } = this.props.block;\n    const stringifiedData = JSON.stringify(data);\n    let dataDisplay =\n      stringifiedData.length > 35\n        ? `${stringifiedData.substring(0, 35)}...`\n        : stringifiedData;\n    if (this.state.displayTransaction) {\n      return (\n        <div>\n          {data.map((transaction) => (\n            <div key={transaction.id}>\n              <hr />\n              <Transaction transaction={transaction} />\n            </div>\n          ))}\n          <br />\n          <Button\n            bsStyle=\"danger\"\n            bsSize=\"small\"\n            onClick={this.toggleTransaction}\n          >\n            Show Less\n          </Button>\n        </div>\n      );\n    }\n    return (\n      <div>\n        <div>Data: {dataDisplay}</div>\n        <Button\n          bsStyle=\"danger\"\n          bsSize=\"small\"\n          onClick={this.toggleTransaction}\n        >\n          Show More\n        </Button>\n      </div>\n    );\n  }\n  render() {\n    const { timestamp, hash, data } = this.props.block;\n    const hashDisplay = `${hash.substring(0, 15)}...`;\n\n    return (\n      <div className=\"Block\">\n        <div>Hash: {hashDisplay}</div>\n        <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\n        {this.displayTransaction}\n      </div>\n    );\n  }\n}\n\nexport default Block;\n"},"lineCount":null}},"error":null,"hash":"f8260e159088ccbe716ecbcad53afa3b","cacheData":{"env":{}}}